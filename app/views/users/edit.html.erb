


 <div class="edit">
<%= form_for @user do |f| %>


<div id="container">  
  <%= f.label :image, :class => 'form-label' %>
  <%= f.url_field :image, :class => 'form-control' %>
 


 <%= f.label :name, :class => 'form-label' %>
  <%= f.text_field :name, :class => 'form-control' %>

<%= f.label :dob, :class => 'form-label' %>
  <%= f.date_field :dob, :class => 'form-control' %>

 
 
 <br><%= f.label :starsign_id, :class => 'form-label' %>
 <br> <%= f.select :starsign_id, Starsign.pluck(:name, :id), :class => 'form-control' %>
</div>

   
  

   <br> <%= f.submit "Submit", :class => 'btn btn-outline-primary' %>
  <% end %>

</div>
